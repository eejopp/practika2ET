<Window x:Class="prac1EF.MainMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:prac1EF"
        mc:Ignorable="d"
        Title="MainMenu" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnBookings" Content="Bookings" HorizontalAlignment="Left" Margin="97,161,0,0" VerticalAlignment="Top" Height="103" Width="176" Click="btnBookings_Click"/>
        <Button x:Name="btnClients"  Content="Clients" HorizontalAlignment="Center" VerticalAlignment="Center" Height="108" Width="168" RenderTransformOrigin="0.546,0.529" Click="btnClients_Click"/>
        <Button x:Name="btnServicess" Content="Servicess" HorizontalAlignment="Left" Margin="532,163,0,0" VerticalAlignment="Top" Height="105" Width="163" RenderTransformOrigin="0.5,0.5" Click="btnServicess_Click">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.22"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
    </Grid>
</Window>
